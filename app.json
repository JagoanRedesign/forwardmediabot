{
  "name": "Man-Userbot",
  "description": "[Man-Userbot] userbot Telegram modular yang berjalan pada Python 3.9+",
  "logo": "https://telegra.ph/file/9dc4e335feaaf6a214818.jpg",
  "keywords": ["telegram", "userbot", "man-userbot", "modular", "python"],
  "repository": "https://github.com/kenkansaja/forwardscoverbot",
  "env": {
    "ADMINS": {
      "description": "Dapatkan API KEY di web my.telegram.org. Masukan yg APP ID Telegram",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Dapatkan Api hash di web my.telegram.org.",
      "required": true
    },
     "GROUP": {
     "description": "isi aja",
     "value": "http://t.me/musikkugroup"
    },
     "CHANNEL": {
     "description": "isi aja",
     "value": "http://t.me/musikkuchannel"
    },
     "DB_PATH": {
      "description": "jangan di ganti",
      "required": false,
      "value": "database/database.db"
    }
  },
  "addons": [
    {
    "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  }
}
